                                        <p class="govuk-body-s">@Resource.lblAmountsArePerHectare</p>

                                        @foreach (var mp in cropManagementPeriods)
                                        {
                                            var rec = Model.Recommendations.Where(r => r.ManagementPeriodID == mp.ID);
                                            @foreach (var recommendation in rec)
