                    @if (!isAutumnNUptakeZero)
                    {
                    <h3 class="govuk-heading-s">@Resource.lblAutumnCropNitrogenNUptake</h3>
                    <p class="govuk-body">@Resource.lblAmountOfNFromTheApplicationTaken.</p>
                    }
                    <h3 class="govuk-heading-s">@Resource.lblEndOfSoilDrainage</h3>
                    <p class="govuk-body">@Resource.lblDateWhenSoilDrainageEnds.</p>


                    <h3 class="govuk-heading-s">@Resource.lblRainfallWithin6HoursOfApplication</h3>
                    <p class="govuk-body">@Resource.lblAmountOfRainThatFallsInThe6Hours.</p>


                    <h3 class="govuk-heading-s">@Resource.lblEffectiveRainfallAfterApplication</h3>
                    <p class="govuk-body">@Resource.lblAverageRainfallBetweenManAppAndEndOfSoilDrainage.</p>


                    <h3 class="govuk-heading-s">@Resource.lblWindspeedDuringApplication</h3>
                    <p class="govuk-body">@Resource.lblWindspeedWhenTheMaterialIsApplied.</p>

                    <h3 class="govuk-heading-s">@Resource.lblTopsoilMoisture</h3>
                    <p class="govuk-body"> @Resource.lblHowMoistTheTopsoilIs.</p">
                </govuk-details-text>
            </govuk-details>

            <table class="govuk-table">
                <tbody class="govuk-table__body">
                    @if (!isAutumnNUptakeZero)
                    {
                        @if (Model.FieldList.Count > 1)
                        {
                            <tr class="govuk-table__row">
                                <td class="govuk-table__cell">@Resource.lblAutumnCropNitrogenUptake</td>
                                <td class="govuk-table__cell">@Resource.lblTheAmountIsDifferentForEachOfTheseFields</td>
                                <td class="govuk-table__cell"><a asp-action="AutumnCropNitrogenUptakeDetail" asp-controller="OrganicManure">@Resource.lblChange</a></td>
                            </tr>
                        }
                        else
                        {
                            <tr class="govuk-table__row">
                                <td class="govuk-table__cell">@Resource.lblAutumnCropNitrogenUptake</td>
                                <td class="govuk-table__cell">@string.Format(Resource.lblAutumnCropNitrogenPerHectare, (Model.AutumnCropNitrogenUptakes[0].AutumnCropNitrogenUptake == 0.0m ? 0 : Model.AutumnCropNitrogenUptakes[0].AutumnCropNitrogenUptake.ToString("#.#")))</td>
                                <td class="govuk-table__cell"><a asp-action="AutumnCropNitrogenUptake" asp-controller="OrganicManure" asp-route-f="@Model.AutumnCropNitrogenUptakes[0].EncryptedFieldId">@Resource.lblChange</a></td>
                            </tr>
                        }
                    }

                    <tr class="govuk-table__row">
                        <td class="govuk-table__cell">@Resource.lblEndOfSoilDrainage</td>
                        <td class="govuk-table__cell">@Model.SoilDrainageEndDate.Value.Date.ToString("dd MMMM yyyy")</td>
                        <td class="govuk-table__cell"><a asp-action="SoilDrainageEndDate" asp-controller="OrganicManure">@Resource.lblChange</a></td>
                    </tr>
                    <tr class="govuk-table__row">
                        <td class="govuk-table__cell">@Resource.lblRainfallWithin6HoursOfApplication</td>
                        <td class="govuk-table__cell">@Model.RainfallWithinSixHours</td>
                        <td class="govuk-table__cell"><a asp-action="RainfallWithinSixHour" asp-controller="OrganicManure">@Resource.lblChange</a></td>
                    </tr>
                    <tr class="govuk-table__row">
                        <td class="govuk-table__cell">@Resource.lblEffectiveRainfallAfterApplication</td>
                        <td class="govuk-table__cell">@string.Format(Resource.lblRainfallMM, Model.TotalRainfall)</td>
                        <td class="govuk-table__cell"><a asp-action="EffectiveRainfall" asp-controller="OrganicManure">@Resource.lblChange</a></td>
                    </tr>
                    <tr class="govuk-table__row">
                        <td class="govuk-table__cell">@Resource.lblWindspeedDuringApplication</td>
                        <td class="govuk-table__cell">@Model.Windspeed</td>
                        <td class="govuk-table__cell"><a asp-action="Windspeed" asp-controller="OrganicManure">@Resource.lblChange</a></td>
                    </tr>
                    <tr class="govuk-table__row">
                        <td class="govuk-table__cell">@Resource.lblTopsoilMoisture</td>
                        <td class="govuk-table__cell">@Model.MoistureType</td>
                        <td class="govuk-table__cell"><a asp-action="TopsoilMoisture" asp-controller="OrganicManure">@Resource.lblChange</a></td>
                    </tr>
                </tbody>
            </table>


            <govuk-button type="submit" class="margin-bottom-forty">
                @Resource.lblContinue
            </govuk-button>
            <p class="govuk-body cancel-link">
                @if (!Model.IsCheckAnswer)
                {
                    <a asp-action="CreateManureCancel" asp-controller="OrganicManure" asp-route-q="@Model.EncryptedFarmId" asp-route-r="@Model.EncryptedHarvestYear">@Resource.lblCancel</a>
                }
                else
                {
                    <a asp-action="CheckAnswer" asp-controller="OrganicManure">@Resource.lblCancel</a>
                }
            </p>
        </form>
    </div>

</div>

