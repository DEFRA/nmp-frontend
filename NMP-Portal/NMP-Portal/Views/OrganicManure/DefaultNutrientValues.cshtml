@using NMP.Portal.Resources
@using NMP.Portal.ViewModels
@using System.Globalization

@model OrganicManureViewModel
@{
    var heading = "";
    if (Model.ManureTypeId == (int)NMP.Portal.Enums.ManureTypes.OtherLiquidMaterials || Model.ManureTypeId == (int)NMP.Portal.Enums.ManureTypes.OtherSolidMaterials)
    {
        heading = string.Format(Resource.lblDoYouWantToUseExistingNutrientValuesFor, Model.OtherMaterialName);
    }
    if (ViewBag.FarmManureApiOption != null && ViewBag.FarmManureApiOption == Resource.lblTrue)
    {
        if (Model.DefaultFarmManureValueDate.HasValue)
        {
            heading = string.Format(Resource.lblStandardNutrientValuesWhwnWeHaveValues, Model.ManureTypeName, Model.DefaultFarmManureValueDate.Value.ToLocalTime().Date.ToString("dd MMMM yyyy", CultureInfo.GetCultureInfo("en-GB")));
        }
    }
    else
    {
        heading = string.Format(Resource.lblStandardNutrientValues, Model.ManureTypeName);
    }

    string title = (!string.IsNullOrEmpty(Model.EncryptedOrgManureId)) ? Resource.lblUpdateOrganicManure : Resource.lblAddOrganicManure;
    ViewData["Title"] = $"{heading} {Resource.lblHyphen} {title}";


    var DefaultNutrientValues = true;
    var NotDefaultNutrientValues = false;
    var backAction = (Model.IsCheckAnswer && Model.IsDefaultNutrientOptionChange) ? "CheckAnswer" : Model.ApplicationMethodCount > 0 ? "ApplicationMethod" : "ManureApplyingDate";

    var unit = "";
    if (Model.IsManureTypeLiquid == null)
    {
        Model.IsManureTypeLiquid = false;
    }
    if (Model.IsManureTypeLiquid != null)
    {
        unit = Model.IsManureTypeLiquid.Value ? Resource.lblValueKgPerMeterCube : Resource.lblValueKgPerTon;
    }

}

@if (ViewBag.Error != null)
{
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <govuk-error-summary>
                <govuk-error-summary-item href="#">@ViewBag.Error</govuk-error-summary-item>
            </govuk-error-summary>
        </div>
    </div>
}
