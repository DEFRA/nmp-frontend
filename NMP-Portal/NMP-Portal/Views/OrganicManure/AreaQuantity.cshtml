                @if (Model.DefoliationList != null && Model.DefoliationList.Count > 0)
                {
                    @for (int i = 0; i < Model.DefoliationList.Count; i++)
                    {

                        <input class="govuk-visually-hidden" type="hidden" asp-for="@Model.DefoliationList[i].CropID" />
                        <input class="govuk-visually-hidden" type="hidden" asp-for="@Model.DefoliationList[i].FieldName" />
                        <input class="govuk-visually-hidden" type="hidden" asp-for="@Model.DefoliationList[i].FieldID" />
                        <input class="govuk-visually-hidden" type="hidden" asp-for="@Model.DefoliationList[i].ManagementPeriodID" />
                        <input class="govuk-visually-hidden" type="hidden" asp-for="@Model.DefoliationList[i].Counter" />
                        <input class="govuk-visually-hidden" type="hidden" asp-for="@Model.DefoliationList[i].DefoliationName" />
                        <input class="govuk-visually-hidden" type="hidden" asp-for="@Model.DefoliationList[i].EncryptedCounter" />
                        <input class="govuk-visually-hidden" type="hidden" asp-for="@Model.DefoliationList[i].Defoliation" />
                    }
                }
            </div>
            @if (Model.IsOrgManureNfieldLimitWarning || Model.IsNMaxLimitWarning || Model.IsEndClosedPeriodFebruaryWarning)
            {
                <govuk-notification-banner>
                    <govuk-notification-banner-title>
                        @Resource.lblImportant
                    </govuk-notification-banner-title>
                    @if (trueWarningCount == 1)
                    {
                        @if (Model.IsOrgManureNfieldLimitWarning)
                        {
                            <p class="govuk-notification-banner__heading">
                                @Model.NmaxWarningPara1
                            </p>
                            <p class="govuk-body">@Model.NmaxWarningPara2</p>
                            @if (Model.NmaxWarningPara3 != null)
                            {
                                <p class="govuk-body">@Model.NmaxWarningPara3</p>
                            }
                        }
                        @if (Model.IsNMaxLimitWarning)
                        {
                            <p class="govuk-notification-banner__heading">
                                @Model.CropNmaxLimitWarningPara1
                            </p>
                            <p class="govuk-body">@Model.CropNmaxLimitWarningPara2</p>
                            <p class="govuk-body">@Model.CropNmaxLimitWarningPara2Additional</p>
                            <p class="govuk-body">@(Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.Wales ? Resource.lblNmaxWalesWarningPara2Extension2 : Resource.lblNmaxEnglandWarningPara2Extension2)</p>
                            <p class="govuk-body">@(Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.Wales ? Resource.lblNmaxWalesWarningPara2Extension3 : Resource.lblNmaxEnglandWarningPara2Extension3)</p>
                            @if (Model.CropNmaxLimitWarningPara3 != null)
                            {
                                <p class="govuk-body">@Model.CropNmaxLimitWarningPara3</p>
                            }
                        }
                        @if (Model.IsEndClosedPeriodFebruaryWarning)
                        {
                            <p class="govuk-notification-banner__heading">
                                @Model.EndClosedPeriodEndFebWarningPara1
                            </p>
                            <p class="govuk-body">@Model.EndClosedPeriodEndFebWarningPara2</p>
                            <p class="govuk-body">@Model.EndClosedPeriodEndFebWarningPara3</p>
                        }
                        @if (Model.IsStartPeriodEndFebOrganicAppRateExceedMaxN150)
                        {
                            <p class="govuk-notification-banner__heading">
                                @Model.StartClosedPeriodEndFebWarningPara1
                            </p>
                            <p class="govuk-body">@Model.StartClosedPeriodEndFebWarningPara2</p>
                            <p class="govuk-body">
                                @Model.StartClosedPeriodEndFebWarningPara3
                            </p>
                        }
                    }
                    @if (trueWarningCount > 1)
                    {
                        <div class="govuk-notification-banner__content">
                            <p class="govuk-notification-banner__heading">
                                @if (Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.England)
                                {
                                    @Resource.lblThisApplicationMayBreachMoreThanOneNVZRule
                                }
                                @if (Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.Wales)
                                {
                                    @Resource.lblThisApplicationMayBreachMoreThanOneWaterResourceRegulation
                                }
                            </p>
                            <ul class="govuk-list govuk-list--bullet">
                                @if (Model.IsOrgManureNfieldLimitWarning)
                                {
                                    <li>@Model.NmaxWarningPara1</li>
                                }
                                @if (Model.IsNMaxLimitWarning)
                                {
                                    <li>@Model.CropNmaxLimitWarningPara1</li>
                                }
                                @if (Model.IsEndClosedPeriodFebruaryWarning)
                                {
                                    <li>@Model.EndClosedPeriodEndFebWarningPara1</li>
                                }
                                @if (Model.IsStartPeriodEndFebOrganicAppRateExceedMaxN150)
                                {
                                    <li>@Model.StartClosedPeriodEndFebWarningPara1</li>
                                }
                            </ul>
                            <govuk-details>
                                <govuk-details-summary>
                                    @if (Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.England)
                                    {
                                        @Resource.lblMoreAboutTheseNVZRules
                                    }
                                    @if (Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.Wales)
                                    {
                                        @Resource.lblMoreAboutTheseWaterResourceRegulation
                                    }
                                </govuk-details-summary>
                                <govuk-details-text>
                                    @if (Model.IsOrgManureNfieldLimitWarning)
                                    {
                                        <h2 class="govuk-heading-s">@Model.NmaxWarningPara1</h2>
                                        <p class="govuk-body">@Model.NmaxWarningPara2</p>
                                    }
                                    @if (Model.IsNMaxLimitWarning)
                                    {
                                        <h2 class="govuk-heading-s">@Model.CropNmaxLimitWarningPara1</h2>
                                        <p class="govuk-body">@Model.CropNmaxLimitWarningPara2</p>
                                        <p class="govuk-body">@Model.CropNmaxLimitWarningPara2Additional</p>
                                        <p class="govuk-body">@(Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.Wales ? Resource.lblNmaxWalesWarningPara2Extension2 : Resource.lblNmaxEnglandWarningPara2Extension2)</p>
                                        <p class="govuk-body">@(Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.Wales ? Resource.lblNmaxWalesWarningPara2Extension3 : Resource.lblNmaxEnglandWarningPara2Extension3)</p>
                                    }
                                    @if (Model.IsEndClosedPeriodFebruaryWarning)
                                    {
                                        <h2 class="govuk-heading-s">@Model.EndClosedPeriodEndFebWarningPara1</h2>
                                        <p class="govuk-body">@Model.EndClosedPeriodEndFebWarningPara2</p>
                                    }
                                    @if (Model.IsStartPeriodEndFebOrganicAppRateExceedMaxN150)
                                    {
                                        <h2 class="govuk-heading-s">@Model.StartClosedPeriodEndFebWarningPara1</h2>
                                        <p class="govuk-body">@Model.StartClosedPeriodEndFebWarningPara1</p>
                                    }
                                </govuk-details-text>
                            </govuk-details>
                            @if (Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.England)
                            {
                                <p class="govuk-body">@Resource.lblItIsAnOffenceToBreachTheNitrateVulnerableZone</p>
                            }
                            @if (Model.FarmCountryId == (int)NMP.Commons.Enums.FarmCountry.Wales)
                            {
                                <p class="govuk-body">@Resource.lblItIsAnOffenceToBreachTheWaterResources</p>
                            }

                        </div>
                    }

                </govuk-notification-banner>
            }
            
            <h1 class="govuk-heading-l">@Resource.lblEnterTheAreaAndQuantity</h1>
            <govuk-input asp-for="@Model.Area" inputmode="numeric" input-class="govuk-input--width-5" spellcheck="false">
                <govuk-input-label is-page-heading="true" class="govuk-label">@Resource.lblAreaSpread
                    <span class="govuk-visually-hidden"> @string.Format(Resource.lblInUnit, Resource.lblhectar)</span>
                </govuk-input-label>
                <govuk-input-suffix>@Resource.lblhectar</govuk-input-suffix>
            </govuk-input>
            <govuk-input asp-for=@Model.Quantity inputmode="numeric" input-class="govuk-input--width-5" spellcheck="false">
                <govuk-input-label is-page-heading="true" class="govuk-label">@Resource.lblQuantityApplied
                    <span class="govuk-visually-hidden"> @string.Format(Resource.lblInUnit, suffixUnit)</span>
                </govuk-input-label>
                <govuk-input-suffix>@suffixUnit</govuk-input-suffix>
            </govuk-input>            
            <govuk-button type="submit" class="margin-bottom-forty" asp-action="AreaQuantity">
                @Resource.lblContinue
            </govuk-button>
            <p class="govuk-body cancel-link">
                @if (!Model.IsCheckAnswer)
                {
                    <a asp-action="CreateManureCancel" asp-controller="OrganicManure" asp-route-q="@Model.EncryptedFarmId" asp-route-r="@Model.EncryptedHarvestYear">@Resource.lblCancel</a>
                }
                else
                {
                    <a asp-action="CheckAnswer" asp-controller="OrganicManure">@Resource.lblCancel</a>
                }
            </p>

        </div>

    </div>
</form>