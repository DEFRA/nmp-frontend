@using NMP.Portal.Resources
@model NMP.Portal.ViewModels.ReportViewModel
@{
    ViewData["Title"] = $"{string.Format(Resource.lblAddLivestockDetailsFor, Model.Year)} {Resource.lblHyphen} {Resource.lblAddLivestockNumber}";
    var yes = Convert.ToBoolean((int)NMP.Portal.Enums.TrueFalse.True);
    var no = Convert.ToBoolean((int)NMP.Portal.Enums.TrueFalse.False);

    var cattle = (int)NMP.Portal.Enums.LivestockGroup.Cattle;
    var pigs = (int)NMP.Portal.Enums.LivestockGroup.Pigs;
    var poultry = (int)NMP.Portal.Enums.LivestockGroup.Poultry;
    var sheep = (int)NMP.Portal.Enums.LivestockGroup.Sheep;
    var goatsDeerOrHorses = (int)NMP.Portal.Enums.LivestockGroup.GoatsDeerOrHorses;

    var occupancyNitrogenOption = "";
    if (Model.OccupancyAndNitrogenOptions == (int)NMP.Portal.Enums.OccupancyNitrogenOptions.UseDefault)
    {
        occupancyNitrogenOption = Resource.lblYesUseTheseValues;
    }
    else if (Model.OccupancyAndNitrogenOptions == (int)NMP.Portal.Enums.OccupancyNitrogenOptions.ChangeOccupancy)
    {
        occupancyNitrogenOption = Resource.lblNoChangeTheOccupancyValue;
    }
    else if (Model.OccupancyAndNitrogenOptions == (int)NMP.Portal.Enums.OccupancyNitrogenOptions.ChangeNitrogen)
    {
        occupancyNitrogenOption = Resource.lblNoChangeTheNitrogenNStandardValue;
    }
    else if (Model.OccupancyAndNitrogenOptions == (int)NMP.Portal.Enums.OccupancyNitrogenOptions.DerogatedFarmChangeBoth)
    {
        occupancyNitrogenOption = Resource.lblNoChangeTheseValues;
    }
    var allEmpty = false;
    if (Model.NumbersInJanuary == null && Model.NumbersInFebruary == null &&
        Model.NumbersInMarch == null && Model.NumbersInApril == null &&
        Model.NumbersInMay == null && Model.NumbersInJune == null &&
        Model.NumbersInJuly == null && Model.NumbersInAugust == null &&
        Model.NumbersInSeptember == null && Model.NumbersInOctober == null &&
        Model.NumbersInNovember == null && Model.NumbersInDecember == null)
    {
        allEmpty = true;
    }
}

@section BeforeContent {
    <govuk-back-link asp-action="BackLivestockCheckAnswer" asp-controller="Report">@Resource.lblBack</govuk-back-link>
}
@if (TempData["ErrorOnLivestockCheckAnswer"] != null)
{
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <govuk-error-summary>
                <govuk-error-summary-item href="#">@TempData["ErrorOnLivestockCheckAnswer"]</govuk-error-summary-item>
            </govuk-error-summary>
        </div>
    </div>
}

