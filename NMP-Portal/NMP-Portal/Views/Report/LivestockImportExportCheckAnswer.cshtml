@using NMP.Portal.Resources
@using System.Globalization
@model NMP.Portal.ViewModels.ReportViewModel
@{
    string action = string.Empty;
    string heading = string.Empty;
    @if (!string.IsNullOrWhiteSpace(Model.EncryptedId))
    {
        heading = Resource.lblUpdateAnOrganicMaterialImportOrExport;
        action = Resource.lblUpdate;
    }
    else
    {
        action = Char.ToUpper(Resource.lblAdd[0]) + Resource.lblAdd.Substring(1);
        heading = string.Format(Resource.lblAddAnImportOrExportForYear,
        Model.Year);
    }

    ViewData["Title"] = $"{heading} {Resource.lblHyphen} {action} {Resource.lblImportsExportsOfLivestockManure}";
    string importExport = "";
    @if (Model.IsImport == null)
    {
        importExport = Enum.GetName(typeof(NMP.Portal.Enums.ImportExport), Model.ImportExport);
    }
    var IsAnyLivestockImportExport = "";
    @if (Model.IsAnyLivestockImportExport != null)
    {
        switch (Model.IsAnyLivestockImportExport)
        {
            case true:
                IsAnyLivestockImportExport = Resource.lblYes;
                break;
            case false:
                IsAnyLivestockImportExport = Resource.lblNo;
                break;
        }
    }
    string unit = "";
    @if (Model.IsManureTypeLiquid != null)
    {
        unit = Model.IsManureTypeLiquid.Value ? Resource.lblCubicMeters : Resource.lbltonnes;
    }
    decimal totalN = 0;
    decimal totalP2O5 = 0;
    var allAddress = new List<string>();

    if (!string.IsNullOrWhiteSpace(Model.Address1)) allAddress.Add(Model.Address1);
    if (!string.IsNullOrWhiteSpace(Model.Address2)) allAddress.Add(Model.Address2);
    if (!string.IsNullOrWhiteSpace(Model.Address3)) allAddress.Add(Model.Address3);
    if (!string.IsNullOrWhiteSpace(Model.Address4)) allAddress.Add(Model.Address4);
    if (!string.IsNullOrWhiteSpace(Model.Postcode)) allAddress.Add(Model.Postcode);
}

@section BeforeContent {
    <govuk-back-link asp-action="BackLivestockImportExportCheckAnswer" asp-controller="Report">@Resource.lblBack</govuk-back-link>
}
@if (TempData["ErrorOnCheckYourAnswers"] != null)
{
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds-from-desktop">
            <govuk-error-summary>
                <govuk-error-summary-item href="#">@TempData["ErrorOnCheckYourAnswers"]</govuk-error-summary-item>
            </govuk-error-summary>
        </div>
    </div>
}
